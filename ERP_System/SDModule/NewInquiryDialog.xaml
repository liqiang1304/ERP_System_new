<Window x:Class="ERP_System.SDModule.NewInquiryDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NewInquiryDialog" Height="300" Width="300">
    <Grid>
        <Label Content="客户名：" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="CustomerNameTextBox" HorizontalAlignment="Left" Height="23" Margin="73,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsReadOnly="True"/>
        <Label Content="货币：" HorizontalAlignment="Left" Margin="19,50,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="CurrencyTextBox" HorizontalAlignment="Left" Height="23" Margin="73,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
